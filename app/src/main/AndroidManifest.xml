<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.ndhzs.demo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TimePlan">

        <activity
            android:name="com.ndhzs.demo.MainActivity3"
            android:screenOrientation="landscape">
        </activity>

        <activity
            android:name="com.ndhzs.demo.MainActivity2"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.ndhzs.demo.MainActivity1"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.ndhzs.demo.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>